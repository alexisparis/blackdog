<?xml version="1.0" encoding="UTF-8"?>
<assembly>
  <id>jar-dist</id>
  <formats>
    <format>dir</format>
  </formats>
  <includeBaseDirectory>false</includeBaseDirectory>
  <fileSets>
      
    <fileSet>
      <directory>target/</directory>
      <outputDirectory>/</outputDirectory>
      <includes>
         <include>*.jar</include>
      </includes>
    </fileSet>
    
  </fileSets>
  <dependencySets>
    <dependencySet>
      <outputDirectory>/libs</outputDirectory>
      <unpack>false</unpack>
      <scope>runtime</scope>
    </dependencySet>
  </dependencySets>
</assembly>