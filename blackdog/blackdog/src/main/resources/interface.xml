        <siberiatoolbar id="toolbar" code="main" AlignmentX="1" orientation="HORIZONTAL"/>
    </panel>
    
    <editorRootDockingPanel id="editorPanel" ConfigurationFilePath="blackdog;1::/docking/configuration.properties">
    
        <dockingSplitPane dividerLocation="0.3" orientation="HORIZONTAL">
            
            <dockingSplitPane dividerLocation="0.8" orientation="VERTICAL">
                <dockingTabbedPane permanent="false"/>
                <dockingTabbedPane permanent="false"/>
            </dockingSplitPane>
            <dockingTabbedPane permanent="false"/>
            
        </dockingSplitPane>
    </editorRootDockingPanel>
    
    <defaultstatusbar id="statusBar" constraints="BorderLayout.SOUTH"/>
    
